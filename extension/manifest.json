{
    "name": "uShare",
    "author": "BILLVOG",
    "description": "uShare, a browser extension that helps you share a url by just scanning a QR Code. Fast. Simple.",
    "version": "1.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png",
        "256": "icons/256x256.png"
    },
    "browser_action": {
        "default_title": "uShare",
        "default_popup": "popup/popup.html"
    },
    "permissions": ["tabs"]
}